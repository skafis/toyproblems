
public class LargestContinousSum {
	public static void main (String[] args){
	int[]hold = new int []{8,1,-9,-4,};
	int sum=0;
	int sum2 =0;
	
	
	for (int i=0; i<hold.length; i++){
		if (hold[i] > 0){
			sum = sum + hold[i];
		}
		else
			sum2 = sum2+hold[i];
			
	}
	if (sum > sum2){
		System.out.print(sum);
		}
	else System.out.print(sum2);
	}
}
